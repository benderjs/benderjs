{
	"name": "benderjs",
	"version": "0.2.3",
	"description": "The anti-human approach to JavaScript testing",
	"keywords": [
		"test",
		"unit",
		"remote",
		"testing",
		"bender",
		"benderjs"
	],
	"homepage": "http://benderjs.com/",
	"scripts": {
		"doc": "jsdoc -r lib/",
		"lint": "jshint lib/ test/ && jscs .",
		"postinstall": "bower install",
		"test": "istanbul test node_modules/mocha/bin/_mocha -- --reporter list --check-leaks --recursive"
	},
	"repository": {
		"type": "git",
		"url": "http://github.com/benderjs/benderjs.git"
	},
	"bugs": {
		"url": "https://github.com/benderjs/benderjs/issues"
	},
	"author": "CKSource (http://cksource.com/)",
	"license": "MIT",
	"dependencies": {
		"bower": "~1.3.12",
		"broadway": "~0.2.9",
		"browser-launcher2": "~0.4.5",
		"chalk": "~1.0.0",
		"connect": "~2.21.1",
		"dom-combiner": "~0.1.1",
		"forever": "~0.11.1",
		"gerard": "~0.2.0",
		"graceful-fs": "~3.0.2",
		"lodash": "~2.4.1",
		"marked": "~0.3.2",
		"mime": "~1.2.11",
		"minimatch": "~1.0.0",
		"ncp": "~0.6.0",
		"nedb": "~0.10.7",
		"node-uuid": "~1.4.1",
